# .txt2phorc Configuration
# --------------------------------------
# PATH variables
# All path names must end with a separator!
DATAPATH=../data/

# Path to voice files
INVPATH=../data/

# Path to temporary and log files (default: /tmp/)
TEMPPATH=/tmp/

# Default Settings
# --------------------------------------
# Prosody
# Possible Values: male, female
INVENTORY=male

# Prosody generation tree
# Possible Values: male, female, newtree
# Best: newtree
NEWTREE=newtree

# Reduction level
REDUCTION=1

# Prominence computation by rule or CART tree
PROMCOMP=1

# Default speech rate
SPEECHRATE=1.0

# Duration computation
# Possible Values: 0-4
# Best: 3
USENET=3
 
# Debuglevel
DEBUGLEVEL=0
